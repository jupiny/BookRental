<ul>
  <% for borrowed_book in @borrowed_books %>
    <li><%= borrowed_book.book_info.bookname %></li>
    <ul>
      <li><%= link_to '상세정보', book_path(borrowed_book) %></li>
      <%= form_for :book, url: check_in_path(borrowed_book), method: :patch do |f| %>
        <%= f.submit "반납하기" %>
      <% end %>
    </ul>
  <% end %>
</ul>

